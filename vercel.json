{
  "version": 2,
  "crons": [
    {
      "path": "/api/cron/watch-leads",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/check-outcomes",
      "schedule": "0 0 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/client/dist/$1"
    }
  ]
}
